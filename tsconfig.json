{
  "compilerOptions": {
    "outDir": "./lib",
    "target": "es2019",
    "lib": ["ES2019"],
    "alwaysStrict": true,
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/**/*.ts"]
}
